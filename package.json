{
  "name": "mdwiki",
  "version": "2.0.0",
  "description": "A simple markdown based wiki",
  "main": "./server/server.js",
  "repository": "",
  "author": "Jan Baer <jan@janbaer.de>",
  "license": "MIT",
  "scripts": {
    "start": "node server/server.js",
    "build": "next build",
    "prod": "NODE_ENV=production node server/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --config=./jest.config.json --forceExit",
    "test:watch": "npm test -- --watch",
    "test:debug": "node --inspect ./node_modules/.bin/jest --runInBand --config=./jest.config.json",
    "precommit": "lint-staged",
    "prepush": "yarn lint && yarn test"
  },
  "engines": {
    "node": ">=8.9.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.2",
    "classnames": "2.2.5",
    "config": "1.28.1",
    "html-to-react": "1.3.1",
    "koa": "2.4.1",
    "koa-bodyparser": "4.2.0",
    "koa-passport": "4.0.1",
    "koa-router": "7.3.0",
    "koa-session": "5.5.1",
    "markdown": "0.5.0",
    "material-ui": "1.0.0-beta.12",
    "material-ui-icons": "1.0.0-beta.17",
    "mobx": "3.3.2",
    "mobx-react": "4.3.5",
    "next": "4.1.4",
    "node-fetch": "1.7.3",
    "passport-github": "1.1.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-simplemde-editor": "3.6.11",
    "react-tap-event-plugin": "3.0.2"
  },
  "devDependencies": {
    "babel-eslint": "8.0.2",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "enzyme": "3.2.0",
    "eslint": "4.12.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.5.1",
    "github-markdown-css": "2.9.0",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "lint-staged": "5.0.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.2.0"
  }
}
