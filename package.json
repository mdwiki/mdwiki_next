{
  "name": "mdwiki",
  "version": "2.0.19",
  "description": "A simple markdown based wiki",
  "main": "./server/server.js",
  "repository": "https://github.com/mdwiki/mdwiki_next",
  "author": "Jan Baer <jan@janbaer.de>",
  "license": "MIT",
  "scripts": {
    "start": "node server/server.js",
    "build": "next build",
    "prod": "NODE_ENV=production node server/server.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest --config=./jest.config.json",
    "test:watch": "npm test -- --watch",
    "test:debug": "node --inspect ./node_modules/.bin/jest --runInBand --config=./jest.config.json",
    "precommit": "lint-staged",
    "prepush": "yarn lint && yarn test"
  },
  "engines": {
    "node": ">=8.9.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "2.2.6",
    "config": "2.0.1",
    "koa": "2.5.2",
    "koa-bodyparser": "4.2.1",
    "koa-passport": "4.1.0",
    "koa-router": "7.4.0",
    "koa-session": "5.8.3",
    "lodash": "4.17.10",
    "markdown-to-jsx": "6.7.1",
    "material-ui": "1.0.0-beta.12",
    "material-ui-icons": "1.0.0-beta.36",
    "mobx": "5.0.4",
    "mobx-react": "5.2.5",
    "next": "6.1.1",
    "node-fetch": "2.2.0",
    "passport-github": "1.1.0",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-scrollchor": "6.0.0",
    "react-simplemde-editor": "3.6.16",
    "react-tap-event-plugin": "3.0.3"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.5",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-preset-env": "1.7.0",
    "enzyme": "3.5.0",
    "eslint": "5.4.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "github-markdown-css": "2.10.0",
    "husky": "0.14.3",
    "jest": "23.5.0",
    "lint-staged": "7.2.2",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.4.2"
  }
}
