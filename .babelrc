{
  "env": {
    "development": {
      "presets": [ "next/babel" ],
      "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        "transform-class-properties"
      ]
    },
    "production": {
      "presets": [ "next/babel" ],
      "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        "transform-class-properties"
      ]
    },
    "test": {
      "presets": [[ "next/babel", { "preset:env": { "modules": "commonjs" }} ]],
      "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        "transform-class-properties"
      ]
    }
  }
}
